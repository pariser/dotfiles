"*":
  Rdio:
    "showEqualizer (WindowResizePerformanceIssue )": false
  aligner:
    ":-alignment": "left"
  "atom-beautify":
    css:
      convert_quotes: "single"
      newline_between_rules: true
    general:
      _analyticsUserId: "b38a6e06-84e0-43a1-9c53-cdebc39b734b"
      analytics: false
  "autocomplete-plus":
    autoActivationDelay: 500
    includeCompletionsFromAllBuffers: true

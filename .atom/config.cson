      ".git"
      ".DS_Store"
      "*~"
      ".git"
      "node_modules"
      ".yardoc"
      "tmp"
      "log"
      "tags"
      "public/system"
      ".tmp"
      ".eslintcache"
      "dist"
      "build"
    ]
    packagesWithKeymapsDisabled: [
      "rails-transporter"
    ]
